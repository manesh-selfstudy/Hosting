{
  "version": "1.0.0-*",
  "compilationOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-*",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-*"
  },
  "frameworks": {
    "net452": {
      "dependencies": {
        "Microsoft.ServiceFabric": "5.0.135",
        "Microsoft.ServiceFabric.AspNetCore.Gateway": "1.0.0-*",
        "Microsoft.ServiceFabric.AspNetCore.Hosting": "1.0.0-*",
        "Microsoft.ServiceFabric.Services": "2.0.135"
      },
      "frameworkAssemblies": {
        "System.Runtime": { "type": "build" },
        "System.ComponentModel": { "type": "build" }
      }
    }
  },
  "runtimes": {
    "win7-x64": { }
  },

  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "content": [
    "appsettings.json"
  ],
  "exclude": [
    "PackageRoot"
  ],
  "publishExclude": [
    "**.user",
    "**.vspscc",
    "PackageRoot"
  ]
}
